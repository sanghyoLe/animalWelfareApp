                        android:layout_height="50dp"
                        android:background="@drawable/survey_number"
                        android:gravity="center_vertical"
                        android:paddingBottom="10dp"
                        android:textColor="@color/colorWhite"
                        android:textSize="17dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/q14"
                        android:layout_width="wrap_content"
                        android:layout_height="90dp"
                        android:layout_marginRight="10dp"
                        android:gravity="top"
                        android:paddingTop="0dp"
                        android:text="@string/calf_straw_title"
                        android:textColor="@color/colorBlack"
                        android:textSize="18dp"
                        android:textStyle="bold" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:orientation="vertical">

                    <RadioGroup
                        android:id="@+id/calf_straw_rg"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="10dp"
                        android:orientation="vertical"
                        android:paddingLeft="20dp">

                        <RadioButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="예-송아지 다리가 마른 깔짚에 반 이상 묻혀있음"
                            android:textSize="15dp"></RadioButton>

                        <RadioButton

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="아니오-깔짚위로 송아지 다리가 많이 보임"
                            android:textSize="15dp"></RadioButton>
                    </RadioGroup>

                </LinearLayout>

            </LinearLayout>
            //여기까지 하나

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/button_circle_line"
                android:orientation="vertical">//여기부터

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/nb15"
                        android:layout_width="40dp"
                        android:layout_height="50dp"
                        android:background="@drawable/survey_number"
                        android:gravity="center_vertical"
                        android:paddingBottom="10dp"

                        android:textColor="@color/colorWhite"
                        android:textSize="17dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/q15"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:layout_marginRight="10dp"
                        android:gravity="top"
                        android:paddingTop="0dp"
                        android:text="@string/calf_warm_title"
                        android:textColor="@color/colorBlack"
                        android:textSize="18dp"
                        android:textStyle="bold" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:orientation="vertical">

                    <RadioGroup
                        android:id="@+id/calf_warm_rg"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="10dp"
                        android:paddingLeft="20dp">

                        <RadioButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="예-따뜻한 보온등을 모든 송아지가 이용가능함"
                            android:textSize="15dp"></RadioButton>

                        <RadioButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="아니오-보온등을 갖추지 아니하였거나 분하지 않음"
                            android:textSize="15dp"></RadioButton>
                    </RadioGroup>

                </LinearLayout>

            </LinearLayout>
            //여기까지 하나

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/button_circle_line"
                android:orientation="vertical">//여기부터

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/nb16"
                        android:layout_width="40dp"
                        android:layout_height="50dp"
                        android:background="@drawable/survey_number"
                        android:gravity="center_vertical"
                        android:paddingBottom="10dp"

                        android:textColor="@color/colorWhite"
                        android:textSize="17dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/q16"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:layout_marginRight="10dp"
                        android:gravity="top"
                        android:paddingTop="0dp"
                        android:text="@string/calf_wind_block_title"
                        android:textColor="@color/colorBlack"
                        android:textSize="18dp"
                        android:textStyle="bold" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:orientation="vertical">

                    <RadioGroup
                        android:id="@+id/calf_wind_block_rg"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="10dp"
                        android:paddingLeft="20dp">

                        <RadioButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="예"
                            android:textSize="15dp"></RadioButton>

                        <RadioButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="아니오"
                            android:textSize="15dp"></RadioButton>
                    </RadioGroup>

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/colorPrimary"
                    android:text="세부기준점수"
                    android:textColor="@color/colorWhite"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/calf_winter_rest_score"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorBlack"
                    android:textSize="16dp" />
            </LinearLayout>
            //여기까지 하나
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="16dp"
                    android:textColor="@color/colorWhite"
                    android:background="@color/colorPrimary"
                    android:text="@string/warm_ventilation_score_title"/>
                <TextView
                    android:id="@+id/warm_ventilation_score"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="16dp"
                    android:textColor="@color/colorBlack"
                    />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.3"
        android:gravity="center_vertical">

        <Button
            android:id="@+id/breed_batch_pre_2_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="이전"
            android:textSize="36sp"
            android:padding="10dp"
            android:layout_margin="10dp"
            android:background="@drawable/button_circle_white"
            android:textColor="@color/colorPrimary"
            android:layout_weight="0.5"/>
        <Button
            android:id="@+id/breed_batch_next_2_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="다음"
            android:textSize="36sp"
            android:padding="10dp"
            android:layout_margin="10dp"
            android:background="@drawable/button_circle"
            android:textColor="@color/colorWhite"
            android:layout_weight="0.5"/>
    </LinearLayout>
</LinearLayout>